{
  "name": "smash",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test --configuration ios.sim.release",
    "test:e2e:build": "detox build --configuration ios.sim.release",
    "eas:login": "eas login",
    "eas:build:android": "eas build --profile preview --platform android",
    "eas:build:ios": "eas build --profile preview --platform ios",
    "eas:build:android:prod": "eas build --profile production --platform android",
    "eas:build:ios:prod": "eas build --profile production --platform ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.8.8",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/native-stack": "^7.8.2",
    "expo": "~54.0.25",
    "expo-blur": "~15.0.8",
    "expo-calendar": "~15.0.8",
    "expo-font": "^14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.6.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.2"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "babel-preset-expo": "^54.0.7",
    "detox": "^20.46.3",
    "detox-expo-helpers": "^0.6.0",
    "eas-cli": "^16.28.0",
    "firebase-tools": "^15.1.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-expo": "^54.0.16"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase)"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.{js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/__tests__/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
